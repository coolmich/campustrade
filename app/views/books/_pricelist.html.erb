<% b.bookprices.order(:price).each do |p| %>
  <div class="panel panel-default price-card">
    <div class="panel-heading">
      <span class="book-condition"><%= p.condition.titleize%></span>
      <span class="book-price">$ <%= p.price%></span> 
      <p class="book-pricetime"><%=time_ago_in_words(p.created_at)%> ago </p>
    </div>
    <div class="panel-body">
      <p><%= p.contact%></p>
    </div>
  </div>
<% end %>