<div class="container" style="margin-top: 100px; text-align: center;">
  <h1>Number of Offers</h1>
  <div class="btn-group" style="margin-top: 30px;">
    <button type="button" class="btn btn-default data-switch" id="to_day">Week</button>
    <button type="button" class="btn btn-default data-switch" id="to_month">Month</button>
    <button type="button" class="btn btn-default data-switch" id="to_season">Season</button>
  </div>
  <div class="week-inspector"></div>
  <%= render 'data', :season => @season_linear, :month => @month_linear, :week=> @week_linear, :week_time => @week_time %>
</div>