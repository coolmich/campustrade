<script>
  var dataset = <%= data %>
  var w = 1000;
  var h = 500;
  var padding = 20;
  var heightScale = 10;
  var svg = d3.select(".user-inspector")
    .append("svg")
    .attr("width", w)
    .attr("height", h);
  var bars = svg.selectAll("rect")
                .data(dataset)
                .enter()
                .append("rect");
  bars.attr("x", function(d, i){ return w/dataset.length * i; })
      .attr("y", function(d){ return h - d * heightScale; })
      .attr("width", function(d, i){ return w/dataset.length - padding; })
      .attr("height", function(d){ return d * heightScale; })
      .attr("fill", "teal");
</script>